{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"cardNumber": {
			"type": "integer"
		},
		"startDate": {
			"type": "string"
		},
		"expiryDate": {
			"type": "string"
		},
		"bookLimit": {
			"type": "integer",
			"minimum": 1,
			"maximum": 999
		},
		"dueAmount": {
			"type": "number"
		}
	},
	"required": [
		"startDate",
		"expiryDate"
	]
}
