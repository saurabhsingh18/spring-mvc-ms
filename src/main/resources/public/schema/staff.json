{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"staffId": {
			"type": "integer"
		},
		"name": {
			"type": "string",
			"minLength": 1
		},
		"role": {
			"type": "string",
			"minLength": 1
		},
		"address": {
			"type": "integer",
			"minimum": 1,
			"maximum": 9999
		},
		"contactnumber": {
			"type": "string",
			"minLength": 1
		}
	},
	"required": [
		"title",
		"author",
		"price",
		"type",
		"language"
	]
}
